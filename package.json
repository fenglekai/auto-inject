{
  "name": "auto-inject",
  "version": "1.20.beta",
  "description": "Custom workflow client.",
  "homepage": "https://vutron.jooy2.com",
  "author": "fenglekai<fenglekai@gmail.com>",
  "license": "MIT",
  "main": "dist/main/index.js",
  "private": true,
  "pkg": {
    "scripts": [
      "tscBuild/koa/**/*.js",
      "tscBuild/main/**/*.js"
    ]
  },
  "scripts": {
    "dev": "ELECTRON_ENABLE_LOGGING=true vite",
    "dev:debug": "vite -d",
    "dev:debug:force": "vite -d --force",
    "dev:server": "nodemon ./src/koa/index.ts",
    "build": "npm run build:pre && electron-builder -wml",
    "build:pre": "npm run format:fix && vue-tsc --noEmit && vite build",
    "build:dir": "npm run build:pre && electron-builder --dir",
    "build:mac": "npm run build:pre && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac",
    "build:linux": "npm run build:pre && electron-builder --linux",
    "build:win": "npm run build:pre && electron-builder --windows",
    "lint": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "test": "npm run build:dir && xvfb-maybe -- playwright test",
    "server:test": "ts-node src/koa/test.ts",
    "pkg:win": "tsc && pkg -t node16-win-x64 -o AutoInject tscBuild/koa/index.js",
    "pkg:linux": "tsc && pkg -t node16-linux-x64 -o AutoInject tscBuild/koa/index.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "axios": "0.27.2",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "jsmodbus": "^4.0.6",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "pinia": "^2.1.4",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "vuetify": "^3.3.6",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@playwright/test": "^1.35.1",
    "@types/koa": "^2.13.7",
    "@types/koa__router": "^12.0.0",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^20.5.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "playwright": "^1.35.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.3",
    "xvfb-maybe": "^0.2.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.2"
  }
}
